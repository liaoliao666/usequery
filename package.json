{
  "name": "vue-query",
  "version": "0.0.0",
  "description": "ðŸŒˆ Composables api for fetching, caching and updating asynchronous data in Vue 3",
  "main": "lib/index.js",
  "author": "liaoliao666",
  "license": "MIT",
  "scripts": {
    "start": "rollup -c -w",
    "build": "yarn build:commonjs && yarn build:es && yarn build:umd",
    "build:commonjs": "rimraf ./lib && cross-env BABEL_ENV=commonjs babel --extensions .ts,.tsx --ignore ./src/**/*.test.tsx ./src --out-dir lib",
    "build:es": "rimraf ./es && babel --extensions .ts,.tsx --ignore ./src/**/*.test.tsx ./src --out-dir es",
    "build:umd": "rimraf ./dist && cross-env NODE_ENV=production rollup -c && rollup-plugin-visualizer stats-vue.json",
    "stats": "open ./stats.html",
    "test": "jest",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:serve": "vitepress serve docs"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.12.3",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-typescript": "^7.12.1",
    "@rollup/plugin-node-resolve": "^10.0.0",
    "@rollup/plugin-replace": "^2.3.4",
    "@types/jest": "^26.0.15",
    "@typescript-eslint/eslint-plugin": "^4.6.1",
    "@typescript-eslint/parser": "^4.6.1",
    "babel-plugin-const-enum": "^1.0.1",
    "babel-plugin-transform-async-to-promises": "^0.8.15",
    "cross-env": "^7.0.2",
    "eslint": "^7.12.1",
    "jest": "^26.6.3",
    "prettier": "^2.1.2",
    "rimraf": "^3.0.2",
    "rollup": "^2.33.1",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-size": "^0.2.2",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-visualizer": "^4.2.0",
    "typescript": "^4.0.5",
    "vitepress": "^0.10.7"
  },
  "dependencies": {
    "@babel/runtime": "^7.12.5",
    "vue": "^3.0.0"
  },
  "peerDependencies": {}
}
